						<li><a href="/p-admin/assets">Media Assets</a></li>
						<% 
							if(pagedata.extensions.menu && pagedata.extensions.menu.Content){
								for (var x in pagedata.extensions.menu.Content){%>
									<li><%- pagedata.extensions.menu.Content[x] %></li>
								<%}
							}
						%>
					</ul>
				</li>
